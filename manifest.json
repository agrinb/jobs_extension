{
  "manifest_version": 2,
  "key": "",
  "name": "Job Alerts",
  "description": "This extension helps you find a job",
  "version": "1.0",
  //"content_security_policy": "script-src http://localhost:3000; object-src 'self'",
  "permissions": [
          "http://*/",
          "activeTab",
          "tabs",
          "identity",
          "https://accounts.google.com/*",
          "https://www.googleapis.com/*",
          "notifications",
          "https://js.pusher.com/*",
          "https://maxcdn.bootstrapcdn.com/*",
          "http://fonts.googleapis.com/*"
          ],
  "oauth2": {
    "client_id": "633513668424-avj1eu477g302go9jc5dd0ff3ll4hgi3.apps.googleusercontent.com",
    "scopes": [ "https://www.googleapis.com/auth/plus.login" ,
                "https://www.googleapis.com/auth/plus.profile.emails.read",
                "https://www.googleapis.com/auth/userinfo.email",
                "email"]
  },
  "content_security_policy": "script-src 'self' https://js.pusher.com; object-src 'self'",
  "browser_action": {
    "default_icon": "job_icon.png",
    "default_popup": "index.html"
  },
  "background": {
    "page": "background.html"
  }
  // "content_scripts": [
